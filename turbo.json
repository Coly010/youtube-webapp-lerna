{
  "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "lint": {
        "outputs": []
      },
      "serve": {
        "cache": false,
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "clean": {
        "cache": false
      },
      "pack": {
        "cache": false
      }
    }
  }
  